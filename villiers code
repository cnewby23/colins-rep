<script>
// Set the date format and start date

$('#Date').datepicker({
  format: 'dd-mm-yyyy',
  daysMin: ['S', 'M', 'T', 'W', 'T', 'F', 'S'], // Set the days to single characters
  weekStart: 1, // Monday as the start of the week
  startDate: new Date()  // block the selection of dates before today
});

// Select the date input field
const dateInput = document.getElementById('Date');

// Add an event listener to handle focus event
dateInput.addEventListener('focus', () => {
  if(window.innerWidth > 1024) { // Check if the viewport width is more than 1024px
    setTimeout(() => {
      // Scroll the input field into view when the calendar is opened
      dateInput.scrollIntoView({ behavior: 'smooth' });
    }, 200);  // Adding delay to allow the calendar to fully open
  }
});

// Add an event listener for pick.datepicker event
$('#Date').datepicker().on('pick.datepicker', function (e) {
  if(window.innerWidth > 1024) { // Check if the viewport width is more than 1024px
    window.scroll({
      top: 0, 
      left: 0, 
      behavior: 'smooth'
    });
  }
});

</script>

<script>
  // Select the date input field
  const dateInput = document.getElementById('Date');

  // Add event listener to handle 'pick' event
  $('#Date').datepicker().on('pick.datepicker', function (e) {
    // Trigger a blur event on the date input field to close the datepicker
    dateInput.blur();
  });
</script>
<script>
window.addEventListener('DOMContentLoaded', (event) => {
  // Check if the device's width is 768 pixels or less
  if (window.innerWidth <= 768) {
    const dateInput = document.getElementById('Date');
    dateInput.setAttribute('readonly', true);
  }
});
</script>

<style>
@media only screen and (max-width: 768px) {
  #Date {
    cursor: default !important;
  }
}
</style>

